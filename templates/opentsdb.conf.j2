tsd.core.auto_create_metrics=true
tsd.core.meta.enable_realtime_ts=true
tsd.core.meta.enable_realtime_uid=true
tsd.core.meta.enable_tsuid_incrementing=false
tsd.core.meta.enable_tracking=true
tsd.core.plugin_path=
tsd.core.tree.enable_processing=false
tsd.http.cachedir=/tmp/opentsdb
tsd.http.request.cors_domains=*
tsd.http.request.enable_chunked=true
tsd.http.request.max_chunk=33554432
tsd.http.show_stack_trace=true
tsd.http.staticroot=/usr/share/opentsdb/static
tsd.network.async_io=true
tsd.network.bind={{ opentsdb_bind }}
tsd.network.keep_alive=true
tsd.network.port={{ opentsdb_port }}
tsd.network.reuse_address=true
tsd.network.tcp_no_delay=true
tsd.network.worker_threads=
tsd.rtpublisher.enable=false
tsd.rtpublisher.plugin=
tsd.search.enable=false
tsd.search.plugin=
tsd.stats.canonical=false
tsd.storage.enable_compaction=true
tsd.storage.flush_interval=1000
tsd.storage.hbase.zk_quorum = {{ zookeeper_quorum }}
######### langerma table-names ###########
tsd.storage.hbase.data_table=tsdb
tsd.storage.hbase.meta_table=tsdb-meta
tsd.storage.hbase.tree_table=tsdb-tree
tsd.storage.hbase.uid_table=tsdb-uid
##########################################
tsd.storage.fix_duplicates=true
tsd.core.socket.timeout = 120
tsd.storage.max_tags=24
######### langerma salting ###############
tsd.storage.salt.width=1
tsd.storage.salt.buckets=32
tsd.query.multi_get.get_all_salts=true
tsd.core.uid.random_metric=true
##########################################
#tsd.storage.enable_appends=true
#tsd.storage.use_otsdb_timestamp=true
#tsd.storage.get_date_tiered_compaction_start=0
tsd.storage.repair_appends=true
tsd.query.allow_simultaneous_duplicates=true
tsd.http.query.allow_delete=true
